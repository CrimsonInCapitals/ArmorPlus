{
  "multipart": [
    {
      "when": {"up": "false", "down": "false"},
      "apply": {"model": "armorplus:block_simple_pillar"}
    },
    {
      "when": {"up": "true", "down": "true"},
      "apply": {"model": "armorplus:block_simple_pillar_mid"}
    },
    {
      "when": {"up": "true", "down": "false"},
      "apply": {"model": "armorplus:block_simple_pillar_bot"}
    },
    {
      "when": {"up": "false", "down": "true"},
      "apply": {"model": "armorplus:block_simple_pillar_top"}
    }
  ]
}